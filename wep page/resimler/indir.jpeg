	<script>
			/*****************************/
			/**** GLOBAL DECLARATIONS ****/
			/*****************************/
			var tools = document.getElementById("tools");
			var toc   = document.getElementById("toc");


			/***********************/
			/**** TOOL BUTTONS ****/
			/***********************/
			var onTools = false;
			var timeoutHandle;

			window.onscroll = function()
			{
				window.clearTimeout(timeoutHandle);
				tools.style.marginRight = "10px";
				
				timeoutHandle = setTimeout(function () {
					if(!showToc && !onTools)
						tools.style.marginRight = "-20%";
				}, 1000);


				// discuss loader
				if ((window.innerHeight + window.scrollY) >= document.getElementById("main-entity").offsetHeight) {
					if (disqus_loaded == false) { load_disqus() };
				}
			}

			document.getElementById("main-entity").onscroll = function()
			{
				window.clearTimeout(timeoutHandle);
				tools.style.marginRight = "10px";
				
				timeoutHandle = setTimeout(function () {
					if(!showToc && !onTools)
						tools.style.marginRight = "-20%";
				}, 1000);
			}

			tools.onmouseenter = function(){
				onTools = true;
			};
			
			tools.onmouseleave = function(){
				onTools = false;
				if(!showToc)
					tools.style.marginRight = "-20%";
			};

			/****************************/
			/**** FULL SCREEN BUTTON ****/
			/****************************/
			var fsMode = false;

			function fsToggle(th) {
				var e = document.getElementById("main-entity");
				
				if (!fsMode) {
					th.innerHtml = "Tam Ekran'dan Çık";
					if (e.requestFullscreen) {
						e.requestFullscreen();
					} else if (e.mozRequestFullScreen) { /* Firefox */
						e.mozRequestFullScreen();
					} else if (e.webkitRequestFullscreen) { /* Chrome, Safari and Opera */
						e.webkitRequestFullscreen();
					} else if (e.msRequestFullscreen) { /* IE/Edge */
						e.msRequestFullscreen();
					}
				} else {
					th.innerHtml = "Tam Ekran";
					document.exitFullscreen();
				}
			}

			document.onfullscreenchange = function ( event ) { 
				fsMode = !fsMode;
			}; 

			/***************************/
			/**** TOGGLE TOC BUTTON ****/
			/***************************/
			var showToc = false;

			function tocToggle(th) {
				
				showToc = !showToc;
				if(showToc)
				{
					toc.className = "visible";
					th.parentElement.className = 'tool-button active';
				}
				else{
					toc.className = "hidden";
					th.parentElement.className = 'tool-button';
				}
			}

			/**********************/
			/**** HOVER DIALOG ****/
			/**********************/
			const hLinks = document.querySelectorAll('[data-description]');
			const hVOffset = 5;
			const hHOffset = 20;
			var hTimeout = 500;
			var onDialog = false;
			const hDialog = document.getElementById('dialog');

			for (var i = 0; i < hLinks.length; i++) {
				hLinks[i].onmouseenter = function () {
					var hTop, hLeft;
					var overWidth = (hDialog.parentElement.clientWidth - this.offsetLeft - 450);

					if (overWidth < 0)
						hLeft = (this.offsetLeft + overWidth - hHOffset) + "px"
					else
						hLeft = this.offsetLeft + "px"


					hDialog.style.top = Math.ceil(this.offsetTop + this.offsetHeight + hVOffset) + "px";
					hDialog.style.left = parseInt(hLeft) < 0 ? "0px" : hLeft;
					hDialog.style.visibility = 'visible';
					hDialog.style.opacity = 1.0;
					document.getElementsByClassName('htitle')[0].innerHTML = this.innerHTML;
					document.getElementsByClassName('htext')[0].innerHTML = this.getAttribute('data-description');
					onDialog = true;
				};
				hLinks[i].onmouseleave = function () {
					setTimeout(function () {
						if (!onDialog) {
							hDialog.style.visibility = 'hidden';
							hDialog.style.opacity = 0;
						}
					}, hTimeout);
					onDialog = false;
				};
			}

			hDialog.onmouseenter = function () { onDialog = true; };
			hDialog.onmouseleave = function () {
				setTimeout(function () {
					if (!onDialog) {
						hDialog.style.visibility = 'hidden';
						hDialog.style.opacity = 0;
					}
				}, hTimeout);
				onDialog = false;
			};

	</script>
