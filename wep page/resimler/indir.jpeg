<script>
    (function(window, document) {
        if ((window.matchMedia && window.matchMedia("(prefers-color-scheme: dark)").matches) || window.localStorage.getItem('theme') === "dark")
         {
          if(window.localStorage.getItem('theme') !== "light"){
            document.documentElement.classList.add('dark');
            document.getElementById('change-colorset').children[0].classList.replace('icon-sun', 'icon-moon-fill');
          }
        }

        if ( window.localStorage.getItem('layout') === 'boxed') {
            document.documentElement.classList.add('boxed');
            document.getElementById('change-layout').children[0].classList.replace('icon-eye', 'icon-sidebar');
        }

        document.addEventListener('DOMContentLoaded', () => {
            const layoutBtn = document.getElementById('change-layout');
            layoutBtn.addEventListener('click', () => {
              const wasFull = document.documentElement.classList.toggle('boxed');
              const layout = wasFull ? 'boxed' : 'full';
                window.localStorage.setItem('layout', layout);

                var iconLayout = layoutBtn.children;
                if (wasFull)
                  iconLayout[0].classList.replace('icon-eye', 'icon-sidebar');
                else
                  iconLayout[0].classList.replace('icon-sidebar', 'icon-eye');
            });

            const toggleButton = document.getElementById('change-colorset');
            toggleButton && toggleButton.addEventListener('click', () => {
                const wasLight = document.documentElement.classList.toggle('dark');
                const theme = wasLight ? 'dark' : 'light';
                window.localStorage.setItem('theme', theme);
				
				var icon = toggleButton.children;
				if (wasLight)
					icon[0].classList.replace('icon-sun', 'icon-moon-fill');
				else
					icon[0].classList.replace('icon-moon-fill', 'icon-sun');
          
            });


        });
    })(window, document);
    </script>

